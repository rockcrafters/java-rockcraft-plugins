plugins {
    id('application')
    id('io.github.rockcrafters.rockcraft')
    id("io.gitlab.plunts.plantuml") version "2.2.0"

}

repositories {
    mavenLocal()
    mavenCentral()
}

dependenciesExport {
    buildScript = false
    configurations = ["runtimeClasspath"]
}

dependencies {
    implementation 'org.springframework.ws:spring-ws-support:4.0.2'
    testImplementation 'org.springframework.ws:spring-ws-support:4.0.3'
}

jar {
    manifest {
        attributes 'Main-Class': 'Test'
    }
}
